<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:types="clr-namespace:Assembly.Helpers.TagEditor.Fields"
                    xmlns:MetaComps="clr-namespace:Assembly.Metro.Controls.PageTemplates.Games.Components.MetaComponents">
	<DataTemplate DataType="{x:Type types:UInt8Field}">
		<MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type types:Int8Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type types:UInt16Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type types:Int16Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type types:UInt32Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
	<DataTemplate DataType="{x:Type types:Int32Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
	</DataTemplate>
    <DataTemplate DataType="{x:Type types:Float32Field}">
        <MetaComps:IntValue Opacity="{Binding Opacity}" />
    </DataTemplate>
</ResourceDictionary>